    "dotenv": "^8.2.0",
    "express": "~4.17.1",
    "firebase": "^7.15.1",
    "firebase-admin": "^8.12.1",
    "got": "^11.3.0",
    "jsdom": "^16.2.2",
    "node-fetch": "^2.6.0",
    "querystring": "~0.2.0",
    "request": "~2.88.2"
  },
  "devDependencies": {
    "@upstatement/eslint-config": "^0.4.3",
    "@upstatement/prettier-config": "^0.3.0",
